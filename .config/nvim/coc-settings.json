{
  "markdownlint.onChange": false,
  "python.linting.enabled": false,
  "python.linting.lintOnSave": false,
  "python.formatting.provider": "black",
  "python.pythonPath": "/usr/local/bin/python3",
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql",
    "py",
    "python",
    "dart",
    "rc",
    "rust"
  ],
  "coc.preferences.extensionUpdateCheck": "daily",
  "python.linting.pylintArgs": [
    "--generate-members"
  ],
  "suggest.floatEnable": true,
  "diagnostic.refreshOnInsertMode": false,
  "flutter.lsp.initialization.closingLabels": true,
  "suggest.timeout": 500,
  "smartf.timeout": 600,
  "smartf.wordJump": false,
  "signature.triggerSignatureWait": 400,
  "rust-analyzer.inlayHints.refreshOnInsertMode": true,
  "coc.preferences.jumpCommand": "tab drop",
  "git.enableGlobalStatus": true,
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": [
        "server",
        "-m",
        "stream"
      ],
      "filetypes": [
        "typescript",
        "typescriptreact",
        "graphql"
      ]
    }
  }
}
