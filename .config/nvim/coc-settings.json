{
  "markdownlint.onChange": false,
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.formatting.provider": "yapf",
  "python.pythonPath": "/usr/local/bin/python3",
  "coc.preferences.formatOnType": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "typescript.ts",
    "json",
    "graphql",
    "py",
    "python",
    "dart",
    "rc",
    "rust",
    "c",
    "cpp",
    "html"
  ],
  "coc.preferences.extensionUpdateCheck": "daily",
  "python.linting.pylintArgs": [
    "--generate-members"
  ],
  "suggest.floatEnable": true,
  "diagnostic.refreshOnInsertMode": false,
  "suggest.timeout": 700,
  "smartf.timeout": 600,
  "smartf.wordJump": false,
  "signature.triggerSignatureWait": 400,
  "rust-analyzer.inlayHints.refreshOnInsertMode": true,
  "rust-analyzer.inlayHints.renderColons": true,
  "rust-analyzer.enable": true,
  "rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": false,
  "coc.preferences.jumpCommand": "tab drop",
  "git.enableGlobalStatus": true,
  "flutter.provider.enableSnippet": true,
  "flutter.lsp.initialization.closingLabels": true,
  "coc.preferences.bracketEnterImprove": false,
  "diagnostic.showUnused": false,
  "languageserver": {
    "cairo": {
            "module": "/usr/local/lib/node_modules/cairo-ls/out/server.js",
            "args": ["--node-ipc"],
            "filetypes": ["cairo"],
            "rootPatterns": [".git/"]
    }
  }
}
